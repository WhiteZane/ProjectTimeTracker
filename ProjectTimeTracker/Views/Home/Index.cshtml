@{
    ViewData["Title"] = "Home Page";
}

<div class="jumbotron">
    <h1>Project Time Tracker</h1>
</div>
<div class="row">
    <div class="col-md-4">
        <h2>Welcome to Time Tracker</h2>
        <p>
            Time tracker allows a user to keep track of the time.
            A database keeps track of the users time by clocking in and out.
            <a asp-area="" asp-controller="Home" asp-action="TrackTime">Select Track Time to start</a> 
            
        </p>
    </div>
    <div class="col-md-4">
        <h2>Team Project</h2>
        <p>Time Tracker is a sample application that
        demonstrates how to use MVC applications
        in a team setting.</p> 
    </div>
    <div class="col-md-4">
        <h2>Download it</h2>
        <p>You can download the completed project from GitHub.</p>
        <p><a class="btn btn-default" href="https://github.com/WhiteZane/ProjectTimeTracker">See project source code &raquo;</a></p>
    </div>
</div>
